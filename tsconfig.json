{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "components/*": ["./components/*"],
      "constants/*": ["./constants/*"],
      "enum/*": ["./enum/*"],
      "hooks/*": ["./hooks/*"],
      "resources/*": ["./resources/*"],
      "services/*": ["./services/*"],
      "style/*": ["./style/*"],
      "types/*": ["./types/*"],
      "utils/*": ["./utils/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "./types"]
}